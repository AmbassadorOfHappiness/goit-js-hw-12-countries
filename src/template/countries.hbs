{{#each this}}
    <h2 class="card-title"> {{name}}</h2>
    <div class="card-body">
        <div>
            <p>Capital: {{capital}}</p>
            <p>Population: {{population}}</p>
            <p>Languages:</p>
            <ul>
                {{#each languages}}
                <li>{{name}}</li>
                {{/each}}
            </ul>
        </div>
        <img src="{{flag}}" alt="Flag of {{name}}" height="250">
    </div>
{{/each}}